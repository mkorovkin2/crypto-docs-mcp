{
  "name": "crypto-docs-mcp",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "clean": "rm -rf packages/*/dist",
    "scraper": "node packages/scraper/dist/index.js",
    "collect-sources-for-project": "node packages/scraper/dist/collect-sources.js",
    "index:generated-docs": "node --loader ts-node/esm packages/scraper/src/manual-generated-indexer.ts",
    "server": "npm run start -w packages/server",
    "dev:server": "npm run dev -w packages/server",
    "test:integration": "node --loader ts-node/esm scripts/test-integration.ts",
    "demo": "npx tsc scripts/demo.ts --outDir scripts --esModuleInterop --module ESNext --moduleResolution node --target ES2022 && node scripts/demo.js",
    "demo:quick": "node scripts/demo.js",
    "demo:bash": "./scripts/demo.sh",
    "eval": "npm run eval -w @crypto-docs/evaluator",
    "eval:mina": "npm run eval -w @crypto-docs/evaluator -- --project mina",
    "eval:solana": "npm run eval -w @crypto-docs/evaluator -- --project solana",
    "eval:cosmos": "npm run eval -w @crypto-docs/evaluator -- --project cosmos",
    "eval:fallback": "npm run eval:fallback -w @crypto-docs/evaluator",
    "eval:verbose": "npm run eval -w @crypto-docs/evaluator -- --verbose",
    "eval:dry-run": "npm run eval -w @crypto-docs/evaluator -- --dry-run",
    "rag-inspector": "npx tsc scripts/rag-inspector.ts --outDir scripts --esModuleInterop --module ESNext --moduleResolution node --target ES2022 && node scripts/rag-inspector.js",
    "rag": "node scripts/rag-inspector.js",
    "rag:mina": "node scripts/rag-inspector.js --project mina",
    "rag:solana": "node scripts/rag-inspector.js --project solana",
    "rag:cosmos": "node scripts/rag-inspector.js --project cosmos",
    "rag:secret": "node scripts/rag-inspector.js --project secret",
    "rag:beam": "node scripts/rag-inspector.js --project beam",
    "rag:pirate-chain": "node scripts/rag-inspector.js --project pirate-chain",
    "rag:polymarket": "node scripts/rag-inspector.js --project polymarket"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "puppeteer": "^24.34.0"
  }
}
