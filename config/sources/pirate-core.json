{
  "id": "pirate-core",
  "type": "github",
  "repoType": "sdk",
  "trustLevel": "official",
  "repo": "PirateNetwork/pirate",
  "branch": "master",
  "scrapeStrategy": {
    "apiPaths": [
      "src/wallet/*.cpp",
      "src/wallet/*.h",
      "src/zcash/*.cpp",
      "src/zcash/*.h",
      "src/primitives/*.cpp",
      "src/primitives/*.h"
    ],
    "exampleDirs": ["doc", "contrib"],
    "exclude": [
      "**/*.test.*",
      "**/test/**",
      "**/tests/**",
      "**/benchmarks/**"
    ],
    "extensions": [".cpp", ".h", ".hpp"]
  },
  "qualityThresholds": {
    "minDocumentationScore": 20,
    "minLLMRelevanceScore": 40,
    "requireReadme": false
  },
  "versionPackages": ["pirate"],
  "description": "Pirate Chain core blockchain - C++ implementation with zk-SNARKs privacy",
  "maintainer": "PirateNetwork",
  "addedAt": "2026-01-11T00:00:00.000Z"
}
