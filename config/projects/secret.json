{
  "id": "secret",
  "name": "Secret Network",
  "docs": {
    "baseUrl": "https://docs.scrt.network",
    "excludePatterns": ["/api-reference/", "/changelog/", "/archive/"],
    "maxPages": 200
  },
  "github": {
    "repo": "scrtlabs/SecretNetwork",
    "branch": "master",
    "include": ["cosmwasm/**/*.rs", "x/**/*.go"],
    "exclude": ["**/*_test.go", "**/test/**", "**/tests/**"]
  },
  "crawler": {
    "concurrency": 3,
    "delayMs": 1000
  }
}
